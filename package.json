{
  "name": "webpack-boilerplate",
  "version": "0.0.1",
  "description": "Quick start for new web application using webpack",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "mocha --compilers js:babel-register",
    "start": "cross-env NODE_ENV=development node server.js",
    "build": "npm run test && cross-env NODE_ENV=production webpack --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/styryshkin/webpack-boilerplate.git"
  },
  "homepage": "https://github.com/styryshkin/webpack-boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "cross-env": "^1.0.7",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "html-webpack-plugin": "^2.8.1",
    "mocha": "^2.4.5",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "author": "Sergey Tyryshkin",
  "license": "MIT"
}
